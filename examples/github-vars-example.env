# Example .env file for GitHub repository variables
# Copy this file to .env and edit as needed before running tasks/setup-github-vars.sh

# Database Configuration
# These will be set as public GitHub variables
DB_USER=postgres
DB_NAME=bun_badges_test
DB_HOST=localhost
DB_PORT=5432

# Sensitive Information 
# These will be set as encrypted GitHub secrets
DB_PASSWORD=your-secure-password-here
TEST_JWT_SECRET=your-test-jwt-secret-here

# Add any other environment variables that should be available in CI
LOG_LEVEL=error
NODE_ENV=test
SKIP_DOCKER=true 